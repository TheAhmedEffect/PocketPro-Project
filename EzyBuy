DROP table ShoppingCart cascade constraints; 
Select* from ShoppingCart;
Select* from Orders;
Select* from Customer;

CREATE OR REPLACE VIEW Customer_Orders AS
SELECT 
    c.User_Id, 
    c.Customer_Name, 
    o.Order_Id, 
    o.Order_Date, 
    o.Order_Amount
FROM 
    Orders o
JOIN 
    ShoppingCart sc ON o.Cart_Id = sc.Cart_Id
JOIN 
    Customer c ON sc.User_Id = c.User_Id;

SELECT* FROM ORDERS;

--Top_Selling
CREATE OR REPLACE VIEW Top_Selling_Products AS
SELECT 
    p.Product_Id, 
    p.Product_Name, 
    SUM(sc.Cart_Item_Quantity) AS Total_Sold
FROM 
    Product p
JOIN 
    ShoppingCart sc ON p.Product_Id = sc.Product_Id
GROUP BY 
    p.Product_Id, p.Product_Name
ORDER BY 
    Total_Sold DESC;


--Low_Stock_Products
CREATE OR REPLACE VIEW Low_Stock_Products AS
SELECT 
    p.Product_Id, 
    p.Product_Name, 
    p.Product_Quantity
FROM 
    Product p
WHERE 
    p.Product_Quantity < 31;

--
CREATE VIEW Revenue_By_Product_Category AS
SELECT 
    pc.Category_Name, 
    SUM(o.Order_Amount) AS Total_Revenue
FROM 
    Product p
JOIN 
    ProductCategory pc ON p.Category_Id = pc.Category_Id
JOIN 
    ShoppingCart sc ON p.Product_Id = sc.Product_Id
JOIN 
    Orders o ON o.Cart_Id = sc.Cart_Id
GROUP BY 
    pc.Category_Name;
 Select* from Orders;

--
CREATE OR REPLACE VIEW Product_Performance_By_Rating AS
SELECT 
    p.Product_Id, 
    p.Product_Name, 
    AVG(r.Rating) AS Average_Rating
FROM 
    Product p
JOIN 
    Review r ON p.Product_Id = r.Product_Id
GROUP BY 
    p.Product_Id, p.Product_Name;

CREATE OR REPLACE VIEW Orders_Without_Payment AS
SELECT 
    o.Order_Id, 
    o.Order_Date, 
    o.Order_Amount
FROM 
    Orders o
LEFT JOIN 
    Payment p ON o.Order_Id = p.Order_Id
WHERE 
    p.Payment_Id IS NULL;


CREATE OR REPLACE VIEW Pending_Orders AS
SELECT 
    o.Order_Id, 
    o.Order_Date, 
    o.Order_Amount
FROM 
    Orders o
LEFT JOIN 
    TrackingDetail td ON o.Order_Id = td.Order_Id
WHERE 
    td.Tracking_No IS NULL;


CREATE OR REPLACE VIEW Payment_History AS
SELECT 
    p.Payment_Id, 
    p.Payment_Date, 
    p.Payment_Amount, 
    p.Payment_Method, 
    o.Order_Id
FROM 
    Payment p
JOIN 
    Orders o ON p.Order_Id = o.Order_Id;


--
CREATE VIEW Customer_Details AS
SELECT 
    c.User_Id, 
    c.Customer_Name, 
    c.Customer_Address, 
    c.Customer_Contact, 
    c.Customer_Mail, 
    u.Password
FROM 
    Customer c
JOIN 
    Users u ON c.User_Id = u.User_Id;

--
CREATE OR REPLACE VIEW Product_Review AS
SELECT 
    r.Review_Id, 
    p.Product_Name, 
    c.Customer_Name, 
    r.Rating, 
    r.Comments, 
    r.Review_Date
FROM 
    Review r
JOIN 
    Product p ON r.Product_Id = p.Product_Id
JOIN 
    Customer c ON r.User_Id = c.User_Id;

CREATE OR REPLACE VIEW Tracking_Order_Status AS
SELECT 
    o.Order_Id, 
    td.Tracking_No, 
    td.Courier_Name
FROM 
    Orders o
LEFT JOIN 
    TrackingDetail td ON o.Order_Id = td.Order_Id;

--
CREATE OR REPLACE VIEW Vip_Customers AS
SELECT 
    c.User_Id, 
    c.Customer_Name, 
    SUM(o.Order_Amount) AS Total_Spent
FROM 
    Customer c
JOIN 
    Orders o ON o.Cart_Id IN (
        SELECT sc.Cart_Id
        FROM ShoppingCart sc
        WHERE sc.User_Id = c.User_Id
    )
GROUP BY 
    c.User_Id, c.Customer_Name
HAVING 
    SUM(o.Order_Amount) > 50000;


--
CREATE OR REPLACE VIEW Most_Active_Customers AS
SELECT 
    c.User_Id, 
    c.Customer_Name, 
    COUNT(o.Order_Id) AS Total_Orders
FROM 
    Customer c
JOIN 
    ShoppingCart sc ON sc.User_Id = c.User_Id
JOIN 
    Orders o ON o.Cart_Id = sc.Cart_Id
GROUP BY 
    c.User_Id, c.Customer_Name
ORDER BY 
    Total_Orders DESC;


Select* from Customer;
