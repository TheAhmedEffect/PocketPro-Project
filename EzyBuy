INSERT INTO Admin (User_Id, Admin_Name, Admin_Role) VALUES ('user809', 'Nasibul', 'Editor');
-- Product Table
CREATE TABLE Product (
    Product_Id INT PRIMARY KEY,
    Product_Name VARCHAR(100),
    Product_Price DECIMAL(10, 2),
    Product_Quantity INT,
    Manufacturer_Id INT,
    Category_Id INT,
    FOREIGN KEY (Manufacturer_Id) REFERENCES Manufacturer(Manufacturer_Id),
    FOREIGN KEY (Category_Id) REFERENCES ProductCategory(Category_Id)
);

CREATE VIEW Kids_Category_View AS
SELECT
    p.Product_Id,
    p.Product_Name,
    p.Product_Price,
    p.Product_Quantity,
    pc.Category_Name,
    m.Manufacturer_Name
FROM 
    Product p
JOIN 
    ProductCategory pc ON p.Category_Id = pc.Category_Id
JOIN 
    Manufacturer m ON p.Manufacturer_Id = m.Manufacturer_Id
WHERE 
    p.Category_Id = 3;

CREATE VIEW Womens_Category_View AS
SELECT
    p.Product_Id,
    p.Product_Name,
    p.Product_Price,
    p.Product_Quantity,
    pc.Category_Name,
    m.Manufacturer_Name
FROM 
    Product p
JOIN 
    ProductCategory pc ON p.Category_Id = pc.Category_Id
JOIN 
    Manufacturer m ON p.Manufacturer_Id = m.Manufacturer_Id
WHERE 
    p.Category_Id = 1;

CREATE VIEW Mens_Category_View AS
SELECT
    p.Product_Id,
    p.Product_Name,
    p.Product_Price,
    p.Product_Quantity,
    pc.Category_Name,
    m.Manufacturer_Name
FROM 
    Product p
JOIN 
    ProductCategory pc ON p.Category_Id = pc.Category_Id
JOIN 
    Manufacturer m ON p.Manufacturer_Id = m.Manufacturer_Id
WHERE 
    p.Category_Id = 2;

CREATE VIEW Cosmetics_Category_View AS
SELECT
    p.Product_Id,
    p.Product_Name,
    p.Product_Price,
    p.Product_Quantity,
    pc.Category_Name,
    m.Manufacturer_Name
FROM 
    Product p
JOIN 
    ProductCategory pc ON p.Category_Id = pc.Category_Id
JOIN 
    Manufacturer m ON p.Manufacturer_Id = m.Manufacturer_Id
WHERE 
    p.Category_Id = 4;


CREATE VIEW Electronics_Category_View AS
SELECT
    p.Product_Id,
    p.Product_Name,
    p.Product_Price,
    p.Product_Quantity,
    pc.Category_Name,
    m.Manufacturer_Name
FROM 
    Product p
JOIN 
    ProductCategory pc ON p.Category_Id = pc.Category_Id
JOIN 
    Manufacturer m ON p.Manufacturer_Id = m.Manufacturer_Id
WHERE 
    p.Category_Id = 5;

CREATE VIEW HomeAppliances_Category_View AS
SELECT
    p.Product_Id,
    p.Product_Name,
    p.Product_Price,
    p.Product_Quantity,
    pc.Category_Name,
    m.Manufacturer_Name
FROM 
    Product p
JOIN 
    ProductCategory pc ON p.Category_Id = pc.Category_Id
JOIN 
    Manufacturer m ON p.Manufacturer_Id = m.Manufacturer_Id
WHERE 
    p.Category_Id = 6;


