Drop table ShoppingCart cascade constraints;
-- Shopping Cart Table
CREATE TABLE ShoppingCart (
    Cart_Id INT PRIMARY KEY,
    Cart_Item_Quantity INT,
    Product_Id INT,
    User_Id VARCHAR2(22),
    FOREIGN KEY (Product_Id) REFERENCES Product(Product_Id),
    FOREIGN KEY (User_Id) REFERENCES Users(User_Id)
);
SELECT* FROM Customer;
INSERT INTO ShoppingCart(Cart_Id, Cart_Item_Quantity, Product_Id, User_Id)
VALUES (2, 1, 16, '12');


-- Order Table
CREATE TABLE Orders(
    Order_Id INT PRIMARY KEY,
    Cart_Id INT,
    Order_Date DATE,
    Order_Amount DECIMAL(10, 2),
    FOREIGN KEY (Cart_Id) REFERENCES ShoppingCart(Cart_Id)
);

-- Payment Table
CREATE TABLE Payment (
    Payment_Id INT PRIMARY KEY,
    Payment_Date DATE,
    Payment_Amount DECIMAL(10, 2),
    Payment_Method VARCHAR(50),
    Order_Id INT,
    FOREIGN KEY (Order_Id) REFERENCES Orders(Order_Id)
);

-- Tracking Detail Table
CREATE TABLE TrackingDetail (
    Tracking_No INT PRIMARY KEY,
    Courier_Name VARCHAR(100),
    Order_Id INT,
    FOREIGN KEY (Order_Id) REFERENCES Orders(Order_Id)
);
INSERT INTO TrackingDetail (Tracking_No, Courier_Name, Order_Id)
VALUES (1, 'Sundarban', 21);
Select* from Orders;
Drop table Review cascade constraints;

-- Review Table
CREATE TABLE Review (
    Review_Id INT PRIMARY KEY,
    Rating INT,
    Comments CLOB,
    Review_Date DATE,
    Product_Id INT,
    User_Id VARCHAR2(22),
    FOREIGN KEY (Product_Id) REFERENCES Product(Product_Id),
    FOREIGN KEY (User_Id) REFERENCES Users(User_Id)
);
