-- User Table
CREATE TABLE Users (
    User_Id VARCHAR2(22) PRIMARY KEY,
    Password VARCHAR(22)
);
INSERT INTO Users (User_Id, Password) VALUES ('user451', '451');

-- Customer Table
CREATE TABLE Customer (
    User_Id VARCHAR2(22) PRIMARY KEY,
    Customer_Name VARCHAR(22),
    Customer_Address VARCHAR(222),
    Customer_Contact VARCHAR(15),
    Customer_Mail VARCHAR(100),
    Order_History CLOB,
    FOREIGN KEY (User_Id) REFERENCES Users(User_Id)
);
INSERT INTO Customer (User_Id, Customer_Name, Customer_Address, Customer_Contact, Customer_Mail, Order_History)
VALUES ('user451', 'Emmy', 'Washington, USA', '987777123456', 'Emmy@gmail.com', 'No');

-- Admin Table
CREATE TABLE Admin (
    User_Id VARCHAR2(22) PRIMARY KEY,
    Admin_Name VARCHAR(100),
    Admin_Role VARCHAR(100),
    FOREIGN KEY (User_Id) REFERENCES Users(User_Id)
);
Select* from Customer;
